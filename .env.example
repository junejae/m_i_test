# MIG is created only on physical GPU 1.
# GPU 0 is currently occupied and excluded from this stack.
MIG_TARGET_GPU_INDEX=1

# Set these after running: nvidia-smi -L
MIG_UUID_1=MIG-REPLACE-WITH-UUID-1
MIG_UUID_2=MIG-REPLACE-WITH-UUID-2

# vLLM settings
MODEL_1=google/gemma-3-12b-it
MODEL_2=Qwen/Qwen3-VL-8B-Instruct
SERVED_MODEL_NAME_1=gemma3-12b-it
SERVED_MODEL_NAME_2=qwen3-vl-8b-instruct
DTYPE_1=bfloat16
DTYPE_2=bfloat16
GPU_MEMORY_UTILIZATION_1=0.85
GPU_MEMORY_UTILIZATION_2=0.85
MAX_MODEL_LEN_1=1024
MAX_MODEL_LEN_2=1024
MAX_NUM_SEQS_1=1
MAX_NUM_SEQS_2=1
MAX_NUM_BATCHED_TOKENS_1=512
MAX_NUM_BATCHED_TOKENS_2=512
MM_IMAGE_LIMIT_2=1
MM_VIDEO_LIMIT_2=0
VLLM_EXTRA_ARGS_1=--swap-space 16 --cpu-offload-gb 10
VLLM_EXTRA_ARGS_2=--swap-space 16 --cpu-offload-gb 8
PORT_1=8101
PORT_2=8102

# Optional
HF_HOME=~/.cache/huggingface
HUGGING_FACE_HUB_TOKEN=
